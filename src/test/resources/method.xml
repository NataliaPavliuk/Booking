            </class>
        </classes>
    </test>
    <test name="checkBasketListD">
        <classes>
            <class name="test.Test">
                <methods>
                    <include name="D"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="checkBasketListE">
        <classes>
            <class name="test.Test">
                <methods>
                    <include name="E"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>